# Example .env file for gosh testing
# Copy this file to .env and adjust the values for your environment

# Test hosts - space-separated list of SSH hostnames
# These should be hosts you can connect to without a password (SSH key auth)
# Use hosts that are safe for read-only testing commands
TEST_HOSTS="xx1w1.local xx1db.local"

# Optional: SSH user to use for connections
# Leave empty to use default SSH behavior (current user or SSH config)
TEST_SSH_USER=""

# Optional: SSH command timeout in seconds (default: 5)
TEST_SSH_TIMEOUT="10"
